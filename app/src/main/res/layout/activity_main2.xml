<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="564dp"
        android:layout_height="423dp"
        android:layout_marginBottom="436dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:srcCompat="@mipmap/naranja"
        tools:ignore="ContentDescription,MissingConstraints,ImageContrastCheck,VisualLintBounds"
        tools:layout_editor_absoluteX="-77dp"
        tools:srcCompat="@mipmap/naranja" />

    <LinearLayout
        android:layout_width="200dp"
        android:layout_height="370dp"
        android:layout_marginTop="430dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3">

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:layout_margin="5dp"
            android:background="#00FFFFFF"
            android:src="@mipmap/basic"
            tools:ignore="ContentDescription,ImageContrastCheck,SpeakableTextPresentCheck"
            tools:src="@mipmap/basic"
            tools:srcCompat="@mipmap/basic" />

        <ImageButton
            android:id="@+id/imageButton13"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:layout_margin="5dp"
            android:background="#00FFFFFF"
            android:src="@mipmap/convers"
            tools:ignore="ContentDescription,SpeakableTextPresentCheck"
            tools:src="@drawable/convers"
            tools:srcCompat="@drawable/convers" />

        <ImageButton
            android:id="@+id/imageButton12"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:layout_margin="5dp"
            android:background="#00FFFFFF"
            android:src="@mipmap/family"
            tools:ignore="ContentDescription,SpeakableTextPresentCheck"
            tools:src="@drawable/family"
            tools:srcCompat="@drawable/family" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="200dp"
        android:layout_height="370dp"
        android:layout_marginTop="430dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3">

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:layout_margin="5dp"
            android:background="#00FFFFFF"
            android:src="@mipmap/ocupa"
            tools:ignore="ContentDescription,SpeakableTextPresentCheck"
            tools:src="@drawable/ocupa"
            tools:srcCompat="@drawable/ocupa" />

        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:layout_margin="5dp"
            android:background="#00FFFFFF"
            android:src="@mipmap/place"
            tools:ignore="ContentDescription,ImageContrastCheck,SpeakableTextPresentCheck"
            tools:src="@drawable/place"
            tools:srcCompat="@drawable/place" />

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:layout_margin="5dp"
            android:background="#00FFFFFF"
            app:srcCompat="@mipmap/food"
            tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck"
            tools:srcCompat="@mipmap/food" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>